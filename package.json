{
	"name": "graffiti-resize",
	"version": "1.0.0",
	"author": "Cavenel Quentin",
	"description": "An image resize app",
	"main": "public/main.js",
	"homepage": "./",
	"build": {
		"extraMetadata": {
			"main": "build/main.js"
		},
		"directories": {
			"buildResources": "build"
		}
	},
	"scripts": {
		"start": "concurrently -k \"cross-env BROWSER=none react-scripts start\" \"build-electron -d\" \"wait-on public/.build-electron-done http://localhost:3000/ && electron .\"",
		"build": "build-electron && react-scripts build",
		"prod": "npm run build && npm exec electron-builder --mac"
	},
	"dependencies": {
		"downloadjs": "^1.4.7",
		"electron-webpack": "^2.8.2",
		"react": "^18.0.0",
		"react-dom": "^18.0.0",
		"react-image-file-resizer": "^0.4.7",
		"react-router-dom": "^6.3.0",
		"source-map-support": "^0.5.16"
	},
	"devDependencies": {
		"@babel/preset-react": "^7.17.12",
		"@types/react": "^18.0.9",
		"babel-loader": "^8.2.5",
		"build-electron": "^1.0.4",
		"concurrently": "^7.2.0",
		"cross-env": "^7.0.3",
		"electron": "^19.0.1",
		"electron-builder": "^23.0.3",
		"electron-is-dev": "^2.0.0",
		"react-scripts": "^5.0.1",
		"wait-on": "^6.0.1",
		"webpack": "^4.46.0"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	}
}
